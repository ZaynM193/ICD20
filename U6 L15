class Main {
  public static void main(String[] args) {
    // Define the range for dice rolls
    int min = 1;
    int max = 6;
    int N;

    // Simulate 10 dice rolls
    for (int i = 0; i < 10; i++) {
      // Generate a random number between min and max
      N = (int) (max * Math.random()) + min;

      // Output the result
      System.out.printf("%d\n", N);
    }
  }
}
